<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mygraduationprojectamit">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:targetSandboxVersion="2"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Mygraduationprojectamit">


        <activity android:name=".seperatedCategory.CategoryContent"></activity>
        <activity android:name=".main.MainActivity" />
        <activity android:name=".productDetails.ProductDetails" />
        <activity android:name=".register.RegisterActivity" />


        <activity
            android:name=".login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>